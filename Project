void drow(string a,string b,string c,string d){
	for(int i=0;i<61;i++){
		cout<<"*";		
	}
	cout<<"\n";
	for(int i=0;i<5;i++){
		for(int i=0;i<65;i++){
			if((i%10)==0){
				cout<<"*";
			}else if(i==64){
				cout<<"\n";
			}else{
				cout<<" ";
			}			
		}
	
	}
	for(int i=0;i<61;i++){
		cout<<"*";		
	}
	cout<<"\n";
	for(int i=0;i<5;i++){	
		for(int i=0;i<5;i++){
			for(int i=0;i<61;i++){
				if(i==0){
					cout<<"*";			
				}else if(i==10){
					cout<<"*";
				}else if(i==50){
					cout<<"*";
				}else if(i==60){
					cout<<"*";
					cout<<"\n";
				}else{
					cout<<" ";
				}
		
			}		
		}
		if(i>=0&&i<=3){
			for(int i=0;i<61;i++){
				if(i>=0&&i<=10){
					cout<<"*";
				}else if(i>=50&&i<=60){
					cout<<"*";			
				}else {
					cout<<" ";
				}

		}
		cout<<"\n";			
	}else{
		
	}

	}
	for(int i=0;i<61;i++){
		cout<<"*";		
	}
	cout<<"\n";
	for(int i=0;i<5;i++){
		for(int i=0;i<65;i++){
			if((i%10)==0){
				cout<<"*";
			}else if(i==64){
				cout<<"\n";
			}else{
				cout<<" ";
			}			
		}
	
	}
		for(int i=0;i<61;i++){
		cout<<"*";		
	}

}
int main(){
	string PlayerName[4];
	for(int i=1;i<5;i++){
	cout<<"PutNamePlayer["<<i<<"]: ";
